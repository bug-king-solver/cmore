{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "24slides/laravel-saml2": "2.2.0",
    "alexwenzel/nova-dependency-container": "1.10",
    "bacon/bacon-qr-code": "2.0.8",
    "barryvdh/laravel-dompdf": "2.0",
    "bavix/laravel-wallet": "^9.6",
    "bavix/laravel-wallet-uuid": "1.0",
    "cartalyst/converter": "7.0.0",
    "darkaonline/l5-swagger": "8.5.1",
    "docusign/esign-laravel": "^1.1",
    "guzzlehttp/guzzle": "7.7.0",
    "hubspot/api-client": "^10.1",
    "kkomelin/laravel-translatable-string-exporter": "1.21.0",
    "lacodix/laravel-model-filter": "1.8.5",
    "laravel/cashier": "13.17.0",
    "laravel/framework": "9.52.10",
    "laravel/helpers": "1.6.0",
    "laravel/nova": "4.26.3",
    "laravel/sanctum": "3.2.5",
    "laravel/slack-notification-channel": "2.5.0",
    "laravel/telescope": "4.15.0",
    "laravel/tinker": "2.8.1",
    "laravel/vonage-notification-channel": "3.2.1",
    "league/flysystem-aws-s3-v3": "3.15.0",
    "livewire/livewire": "2.12.3",
    "outl1ne/nova-multiselect-field": "4.3.5",
    "phpoffice/phpspreadsheet": "1.29.0",
    "pragmarx/google2fa-laravel": "2.1.1",
    "pragmarx/recovery": "0.2.1",
    "sentry/sentry-laravel": "3.6.1",
    "sixlive/nova-text-copy-field": "1.5.0",
    "spatie/laravel-activitylog": "4.7.3",
    "spatie/laravel-comments": "1.4.4",
    "spatie/laravel-comments-livewire": "1.4.0",
    "spatie/laravel-csp": "2.8.3",
    "spatie/laravel-enum": "3.0.2",
    "spatie/laravel-medialibrary": "10.15.0",
    "spatie/laravel-permission": "5.10.2",
    "spatie/laravel-translatable": "6.5.2",
    "spatie/nova-translatable": "3.1.2",
    "stancl/tenancy": "3.7.0",
    "symfony/http-client": "6.3.1",
    "symfony/intl": "6.3.1",
    "symfony/postmark-mailer": "6.3.0",
    "tightenco/nova-stripe": "v1.0.0",
    "tio/laravel": "1.23",
    "visanduma/nova-two-factor": "2.2.7",
    "wire-elements/modal": "1.0.8"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "3.8.1",
    "barryvdh/laravel-ide-helper": "2.13.0",
    "beyondcode/laravel-query-detector": "1.7.0",
    "fakerphp/faker": "1.23.0",
    "friendsofphp/php-cs-fixer": "3.21.1",
    "jasonmccreary/laravel-test-assertions": "2.3",
    "laradumps/laradumps": "2.2.0",
    "larastan/larastan": "^2.6",
    "laravel-shift/factory-generator": "dev-master",
    "mockery/mockery": "1.6.2",
    "nunomaduro/collision": "6.4",
    "phpstan/phpstan": "1.10.6",
    "phpunit/php-code-coverage": "9.2.26",
    "phpunit/phpunit": "9.6.9",
    "rector/rector": "^0.14.6",
    "spatie/laravel-ignition": "1.6.4",
    "tightenco/duster": "^2.6"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": false,
      "phpstan/extension-installer": false
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    },
    {
      "type": "composer",
      "url": "https://satis.spatie.be"
    },
    {
      "type": "composer",
      "url": "https://packages.cartalyst.com"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "sniff": [
      "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
    ],
    "lint": [
      "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
    ]
  }
}
